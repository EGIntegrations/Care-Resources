{
  "expo": {
    "name": "Care Resources",
    "slug": "care-resources",
    "version": "1.0.6",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0C2340"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.careresources.app",
      "buildNumber": "5",
      "googleServicesFile": "./assets/GoogleService-Info.plist",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Use Face ID or Touch ID to access your secure care resources",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0C2340"
      },
      "package": "com.careresources.app",
      "versionCode": 2,
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-local-authentication",
      [
        "expo-av",
        {
          "microphonePermission": false
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "ios": {
            "googleServicesFile": "./assets/GoogleService-Info.plist"
          }
        }
      ],
      "@react-native-firebase/auth",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.1041606516821-3srnqsjb1pbp0p8jsq96ma1oskefb5se"
        }
      ],
      "expo-asset",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "3f46ff14-fe77-4cde-a8d2-9d193d247f62"
      }
    },
    "cli": {
      "appVersionSource": "local"
    }
  }
}
